{
  "private": true,
  "scripts": {
    "lint": "eslint *.js src tools server",
    "relay-compiler": "relay-compiler --watch --src ./src --schema ./data/schema.graphql",
    "update-schema": "babel-node tools/updateSchema.js",
    "precommit": "lint-staged",
    "dev": "npm run clean && cross-env NODE_ENV=development node server/index.js",
    "prod": "npm run clean && cross-env NODE_ENV=production node server/index.js",
    "clean": "rimraf buildClient buildServer"
  },
